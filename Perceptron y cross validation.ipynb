    "from random import seed\n",
    "from random import randrange\n",
    "def Cross_validate_split(D,k):\n",
    "    \"\"\"Esta funcion toma un dataset y un numero de particiones y devuelve las particiones de dataset para entrenamiento\n",
    "       input:D que es el dataset y k que es el numero de particiones deseadas\n",
    "       output: para cada iteracion del for externo, devuelve una particion de entrenamiento\n",
    "       Cota asintotica de tiempo= O=n**2\n",
    "       \"\"\"\n",
    "    D_split=[]\n",
    "    D_copy=D \n",
    "    tamaño_particion=int(len(D)/k)#1\n",
    "    for i in range(k):#guarda cada particion en una lista #n \n",
    "        particion=list()\n",
    "        while len(particion) < tamaño_particion:#ciclo while interno para agregar elementos a la lista\n",
    "                random = randrange(len(D_copy))#elige al azar un elemento\n",
    "                index = D_copy.index[random]# determina el indice \n",
    "                particion.append(D_copy.loc[index].values.tolist())#agrega los elementos a la lista vacia\n",
    "                D_copy = D_copy.drop(index)#elimina los elementos con ese indice \n",
    "                D_split.append(np.asarray(particion))#guarda la particion\n",
    "        return D_split    "
   ]
